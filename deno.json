{
  "name": "@api-lense/api-lense-math",
  "version": "0.1.0",
  "exports": "./mod.ts",
  "license": "MIT",
  "tasks": {
    "dev": "deno test --watch",
    "lint": "deno lint",
    "lint:fix": "deno lint --fix",
    "fmt": "deno run -A npm:prettier --write .",
    "hook": "deno run --allow-read --allow-run --allow-write https://deno.land/x/deno_hooks@0.1.1/mod.ts",
    "test": "deno test --allow-read --allow-env --allow-net --ignore=tests/integration"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1"
  },
  "compilerOptions": {
    "strict": true
  },
  "lint": {
    "files": {
      "include": ["."],
      "exclude": ["node_modules/", "dist/", "build/", ".git/"]
    },
    "rules": {
      "tags": ["recommended"],
      "exclude": ["no-explicit-any", "ban-untagged-todo"]
    }
  },
  "bench": {
    "include": [
      "tests/benchmarks/**/*.benchmark.ts",
      "tests/benchmarks/**/*{.,_,-}bench.{ts,tsx,js,jsx,mjs,cjs}"
    ]
  }
}
